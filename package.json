{
  "name": "timesheet-cli",
  "version": "0.1.0",
  "private": true,
  "homepage": "/",
  "dependencies": {
    "@material-ui/core": "^4.9.13",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/styles": "^4.9.13",
    "@xstate/react": "^0.8.1",
    "axios": "^0.19.2",
    "firebase": "^7.14.2",
    "firebase-admin": "^8.11.0",

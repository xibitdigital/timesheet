    "@typescript-eslint/eslint-plugin": "2.18.0",
    "@typescript-eslint/parser": "^2.32.0",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.2",
    "eslint": "6.8.0",
    "eslint-config-airbnb-typescript": "6.3.1",
    "eslint-config-prettier": "6.10.0",
